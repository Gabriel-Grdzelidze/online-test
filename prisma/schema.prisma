generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Student{
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String   @unique
  password  String
  idNumber String @unique  
  scode Int
}

model Question{
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  question String
  options String[]
  correct String
}

